{
  "name": "thinkeep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi": "openapi-generator-cli generate",
    "svg": "yarn run svg:build && yarn run svg:clean && yarn run svg:registry",
    "svg:build": "yarn svgr public/icon --out-dir src/components/ui/svg-icon/templates --typescript",
    "svg:clean": "yarn rimraf public/icon/*.svg",
    "svg:registry": "yarn tsx src/utils/generate-svg-registry.ts"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.21.2",
    "@svgr/cli": "^8.1.0",
    "next": "15.4.1",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@yarnpkg/pnpify": "^4.1.5",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2"
}
